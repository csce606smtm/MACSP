          <td>
            <%#= link_to(qsheets_path, :html => {:class => 'btn btn-default btn-med'}) do %>
            <span class='glyphicon glyphicon-trash'></span>
          </td>
          -->
         </tr>
        <% end %>
    </tbody>
  </table>
</div>
